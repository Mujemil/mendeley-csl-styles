<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.0">
  <info>
    <title>Harvard-Elsevier-Italic</title>
    <id>http://www.zotero.org/styles/harvard-elsevier-italic</id>
    <link href="http://www.zotero.org/styles/harvard" rel="self"/>
    <author>
      <name>OpenAI GPT Mod</name>
    </author>
    <category citation-format="author-date"/>
    <updated>2025-04-29T00:00:00+00:00</updated>
  </info>
  <citation>
    <layout prefix="(" suffix=")" delimiter=", ">
      <text variable="author"/>
      <text variable="issued" prefix=" " suffix=""/>
    </layout>
  </citation>
  <bibliography>
    <layout hanging-indent="true" delimiter=". ">
      <text variable="author"/>
      <text variable="issued" prefix=" (" suffix=")"/>
      <text variable="title" suffix=". "/>
      <text variable="container-title" font-style="italic" suffix=". "/>
      <text variable="volume" prefix="Vol " suffix=", "/>
      <text variable="page"/>
    </layout>
  </bibliography>
</style>
